        },
        {
          "id": "zqlogs",
          "name": "调试日志",
          "val": false,
          "type": "boolean",
          "desc": "调试日志开关，默认关闭"
        },
        {
          "id": "currentAccIndex",
          "name": "当前账号序号",
          "val": "1",
          "type": "number",
          "desc": "需要获取第几个账号的Cookie就填写对应的数字，如第1个账号填写1，第2个账号就填写2，依次类推(默认从1开始)。"
        },
        {
          "id": "totalAcc",
          "name": "账号总数",
          "val": "1",
          "type": "number",
          "desc": "总共有几个账号，填写数字。"
        }
      ],
